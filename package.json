{
  "name": "fineract-typescript-client",
  "version": "1.0.0",
  "description": "TypeScript client for Fineract API",
  "main": "demo-typescript.ts",
  "scripts": {
    "demo": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx ts-node demo.ts",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx ts-node demo.ts",
    "generate-typescript-client": "JAVA_OPTS=\"-Xmx4G\" openapi-generator-cli generate -i ./fineract.json -g typescript-axios -o ./src/typescript-client --additional-properties=usePromises=true,withInterfaces=true,supportsES6=true,npmName=fineract-typescript-client",
    "regenerate-typescript-client": "rm -rf ./src/typescript-client && npm run generate-typescript-client"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "fineract",
    "typescript",
    "api-client",
    "microfinance"
  ],
  "author": "",
  "license": "MIT"
}
